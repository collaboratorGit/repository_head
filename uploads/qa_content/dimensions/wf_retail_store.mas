FILENAME=WF_RETAIL_STORE, SUFFIX=SQLPSTGR, TRANS_FILE=_EDAHOME/NLS/dt, BV_NAMESPACE=OFF,
 REMARKS='Store InfoHold MFD, format BINARY', $
 VARIABLE NAME=&&STORE_ID,  PROMPT='Select Store Name', USAGE=I9,
    SELECT=MULTI, SELECTOP=OR,
    ACCEPT=SYNONYM (ID_STORE AS STORE_NAME IN QA_REPOSITORY_HEAD/UPLOADS/QA_CONTENT/DIMENSIONS/WF_RETAIL_STORE), $
  SEGMENT=WF_RETAIL_STORE, SEGTYPE=S0,
    DESCRIPTION='Store Info Dimension', $
    FIELDNAME=ID_STORE, ALIAS=ID_STORE, USAGE=I9, ACTUAL=I4,
      TITLE='ID Store', DESCRIPTION='Primary Key', $
    FIELDNAME=ID_GEOGRAPHY, ALIAS=ID_GEOGRAPHY, USAGE=I9, ACTUAL=I4,
      MISSING=ON,
      TITLE='ID Geography', DESCRIPTION='Foreign Key', $
    FIELDNAME=ID_OPEN_DATE, ALIAS=ID_OPEN_DATE, USAGE=I9, ACTUAL=I4,
      MISSING=ON,
      TITLE='ID Open Date', DESCRIPTION='Foreign Key', $
    FIELDNAME=ID_CLOSE_DATE, ALIAS=ID_CLOSE_DATE, USAGE=I9, ACTUAL=I4,
      MISSING=ON,
      TITLE='ID Close Date', DESCRIPTION='Foreign Key', $
    FIELDNAME=STORE_TYPE, ALIAS=STORE_TYPE, USAGE=A11V, ACTUAL=A11V,
      MISSING=ON,
      TITLE='Store Type', DESCRIPTION='Store Type Description', $
    FIELDNAME=AREA_RANGE_SQ_FT, ALIAS=AREA_RANGE_SQ_FT, USAGE=A20V, ACTUAL=A20V,
      MISSING=ON,
      TITLE='Area,Range,Sq. Feet', DESCRIPTION='Area Range in Square Feet', $
    FIELDNAME=AREA_RANGE_SQ_M, ALIAS=AREA_RANGE_SQ_M, USAGE=A20V, ACTUAL=A20V,
      MISSING=ON,
      TITLE='Area,Range,Sq. Meters', DESCRIPTION='Area Range in Square Meters', $
    FIELDNAME=AREA_SQ_FT, ALIAS=AREA_SQ_FT, USAGE=I9C, ACTUAL=I4,
      MISSING=ON,
      TITLE='Area, Sq. Feet', DESCRIPTION='Area in Square Feet', $
    FIELDNAME=AREA_SQ_M, ALIAS=AREA_SQ_M, USAGE=I9C, ACTUAL=I4,
      MISSING=ON,
      TITLE='Area,Sq. Meters', DESCRIPTION='Area in Square Meters', $
    FIELDNAME=AVERAGE_NUMBER_EMPLOYEES, ALIAS=AVERAGE_NUMBER_EMPLOYEES, USAGE=I5C, ACTUAL=I4,
      MISSING=ON,
      TITLE='Average,Number,Employee', DESCRIPTION='Average Number of Employees', $
    FIELDNAME=EXPECTED_SALES, ALIAS=EXPECTED_SALES, USAGE=D20.2M, ACTUAL=D8,
      MISSING=ON,
      TITLE='Expected,Sales', DESCRIPTION='Expected Sales', $
    FIELDNAME=NUMBER_CASH_REGISTER, ALIAS=NUMBER_CASH_REGISTER, USAGE=I9, ACTUAL=I4,
      MISSING=ON,
      TITLE='Number,of Cash,Registers', DESCRIPTION='Number of Cash Registers', $
    FIELDNAME=RENT, ALIAS=RENT, USAGE=D20.2M, ACTUAL=D8,
      MISSING=ON,
      TITLE='Yearly,Rent', DESCRIPTION='Yearly Rent for Store', $
    FIELDNAME=SIZE, ALIAS=SIZE, USAGE=A6V, ACTUAL=A6V,
      MISSING=ON,
      TITLE='Size', DESCRIPTION='Size of Store: Big, Medium, Small', $
    FIELDNAME=STORE_LAYOUT_FORMAT, ALIAS=STORE_LAYOUT_FORMAT, USAGE=A20V, ACTUAL=A20V,
      MISSING=ON,
      TITLE='Store,Layout,Format', DESCRIPTION='Store Layout Format', $
    FIELDNAME=STORE_NAME, ALIAS=STORE_NAME, USAGE=A30V, ACTUAL=A30V,
      MISSING=ON,
      TITLE='Store Name', DESCRIPTION='Store Name', $
    FIELDNAME=UTILITIES, ALIAS=UTILITIES, USAGE=D20.2M, ACTUAL=D8,
      MISSING=ON,
      TITLE='Yearly,Utilities', DESCRIPTION='Yearly Utilities for Store', $
    FILTER STORE_PROMPT=ID_STORE EQ &&STORE_ID;
      TITLE='Store Name', DESCRIPTION='Store Filter Using STORE_ID Prompt', $
    FILTER WEB_STORE=ID_STORE EQ 0;
      TITLE='Web Store', DESCRIPTION='Web Store Filter', $
    FILTER STORE_FRONT=ID_STORE NE 0;
      TITLE='Store Front', DESCRIPTION='Store Front Filter', $
